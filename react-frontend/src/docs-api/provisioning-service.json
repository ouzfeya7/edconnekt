        "/provisioning/batches/{batch_id}/test-items": {
            "post": {
                "tags": [
                    "provisioning",
                    "provisioning"
                ],
                "summary": "Create Test Item",
                "description": "Crée un item de test pour un batch (pour les tests uniquement).\n\nArgs:\n    batch_id: ID du batch\n    item_data: Données de l'item avec les clés:\n        - identity_id: UUID de l'identité\n        - firstname: Prénom\n        - lastname: Nom\n        - email: Email\n        - role_principal_code: Rôle principal\n        - establishment_id: ID de l'établissement (optionnel)",
                "operationId": "create_test_item_provisioning_batches__batch_id__test_items_post",
                "parameters": [
                    {
                        "name": "batch_id",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "format": "uuid",
                            "title": "Batch Id"
                        }
                    }
                ],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "additionalProperties": true,
                                "title": "Item Data"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    },
                    "422": {
                        "description": "Validation Error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/HTTPValidationError"
                                }
                            }
                        }
                    }
                }
            }
