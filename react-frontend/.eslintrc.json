{
  "extends": [
    "@eslint/js"
  ],
  "rules": {
    "@typescript-eslint/no-unused-vars": "warn",
    "@typescript-eslint/no-explicit-any": "warn",
    "react-hooks/exhaustive-deps": "warn",
    "react-refresh/only-export-components": "warn",
    "no-empty": "warn",
    "no-case-declarations": "warn"
  },
  "ignorePatterns": [
    "dist",
    "node_modules"
  ]
} 